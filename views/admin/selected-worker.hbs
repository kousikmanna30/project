<div id="layoutSidenav">
    {{>admin/sidebar}}
    <div id="layoutSidenav">
        <div id="layoutSidenav_content">
            <main>
                <div class="container-fluid px-4">
                    <h2 class="mt-4">Worker profile</h2>
                    <div class="card mb-5">
                        <div class="card-body">
                            <table class="application-table">
                                 <thead>
                                <tr>
                                    <th>Name</th>
                                    <th>Email</th>
                                </tr>
                            </thead>
                            <tbody>
                                {{#each data}}
                                <tr>
                                    <td>{{name}}</td>
                                    <td>{{email}}</td>
                                </tr>
                                {{/each}}

                            </table>
                        </div>
                    </div>
                </div>
            </main>
        </div>
    </div>
</div>